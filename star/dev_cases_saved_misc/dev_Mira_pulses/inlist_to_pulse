&controls

      !log_L_upper_limit = 3.8

      ! limit max_model_number as part of test_suite
      !max_model_number = 14461

! atm
   use_momentum_outer_BC = .true.

! mlt
   alpha_semiconvection = 0.0

! mesh
   mesh_delta_coeff = 1.25
   
! timesteps
   delta_lgTeff_limit = 0.01d0
   delta_lgTeff_hard_limit = 0.05d0
   relax_hard_limits_after_retry = .false.

   delta_lgL_H_limit = 0.05 ! limit for magnitude of change in lgL_H
   delta_lgL_H_hard_limit = 0.5
   lgL_H_burn_min = 1.5 ! ignore changes in lgL_H if value is less than this

   delta_lgL_He_limit = 0.025 ! limit for magnitude of change in lgL_He
   delta_lgL_He_hard_limit = 1
   lgL_He_burn_min = 2.5 ! ignore changes in lgL_He if value is less than this

! solver

! GYRE output controls
   x_integer_ctrl(1) = -25 ! output GYRE info at this step interval
   x_logical_ctrl(1) = .false. ! save GYRE info whenever save profile
   x_integer_ctrl(2) = 2 ! max number of modes to output per call
   x_logical_ctrl(2) = .false. ! output eigenfunction files   
   x_ctrl(2) = 1 ! must be > 0 for GYRE to report growth information
   x_integer_ctrl(3) = 0 ! mode_l
   x_integer_ctrl(4) = -1 ! report info for min growth/period for this order
      ! < 0 means report best over all gyre results for any order

! output controls
   num_trace_history_values = 2
   trace_history_value_name(1) = 'rel_E_err'
   trace_history_value_name(2) = 'log_rel_run_E_err'
   

   photo_interval = 100
   profile_interval = 100
   history_interval = 25
   terminal_interval = 25

