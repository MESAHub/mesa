&pgstar

   !pause = .true.

   ! HR window -- lg_L vs. lg_Teff
   
   pgstar_interval = 25

   HR_win_flag = .true.
   
   HR_title = '2 M\d\(2281)\u  Z=0.02  Mira'

   HR_win_width = 8
   HR_win_aspect_ratio = 0.62 ! aspect_ratio = height/width

   ! axis limits -- to override system default selections
  HR_logT_min = 3.35 ! -101 ! only used if > -100
  HR_logT_max = 3.57 ! only used if > -100
  HR_logL_min = 2.9 ! -101 ! only used if > -100
  HR_logL_max = 4.0 ! -101 ! only used if > -100

   show_HR_classical_instability_strip = .true.
   !show_HR_target_box = .true.
   HR_target_n_sigma = -3
   HR_target_logL = 4.9d0
   HR_target_logL_sigma = 0.04d0
   HR_target_logT = 4.4d0
   HR_target_logT_sigma = 0.02d0

   ! file output
   !HR_file_flag = .true.
   HR_file_dir = 'pgstar_out'
   HR_file_prefix = 'hr'
   HR_file_interval = 1 ! output when mod(model_number,HR_file_interval)==0
   HR_file_width = -1 ! negative means use same value as for window
   HR_file_aspect_ratio = -1 ! negative means use same value as for window
         
   ! window 2
   
   History_Panels2_win_flag = .true.
   History_Panels2_win_width = 7
   History_Panels2_win_aspect_ratio = 0.75
   
   History_Panels2_title = '2 M\d\(2281)\u  Z=0.02  Mira'      
   History_Panels2_num_panels = 2

   !History_Panels2_xaxis_name='model_number'
   History_Panels2_xaxis_name='star_age'
   History_Panels2_automatic_star_age_units = .true.
   History_Panels2_max_width = -1 !  200 ! only used if > 0.  causes xmin to move with xmax.
   
   History_Panels2_yaxis_name(1) = 'log_L' 
   History_Panels2_yaxis_reversed(1) = .false.
   History_Panels2_ymin(1) = -101d0 ! only used if /= -101d0
   History_Panels2_ymax(1) = -101d0 ! only used if /= -101d0        
   History_Panels2_dymin(1) = -1 
   
   History_Panels2_other_yaxis_name(1) = 'Teff' 
   History_Panels2_other_yaxis_reversed(1) = .false.
   History_Panels2_other_ymin(1) = -101d0 ! only used if /= -101d0
   History_Panels2_other_ymax(1) = -101d0 ! only used if /= -101d0        
   History_Panels2_other_dymin(1) = -1 
  
  History_Panels2_yaxis_name(2) = 'log_R' 
  History_Panels2_yaxis_reversed(2) = .false.
  History_Panels2_ymin(2) = -101d0 ! only used if /= -101d0
  History_Panels2_ymax(2) = -101d0 ! only used if /= -101d0        
  History_Panels2_dymin(2) = -1 
  
  History_Panels2_other_yaxis_name(2) = 'model_number' 
  History_Panels2_other_yaxis_reversed(2) = .false.
  History_Panels2_other_ymin(2) = -101d0 ! only used if /= -101d0
