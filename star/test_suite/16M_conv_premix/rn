#!/bin/bash

# uncomment the following line to run all parts of the test
# MESA_RUN_OPTIONAL=t

# this provides the definition of do_one (run one part of test)
# do_one [inlist] [output model] [LOGS directory]
MESA_DIR=../../..
source "${MESA_DIR}/star/test_suite/test_suite_helpers"

date "+DATE: %Y-%m-%d%nTIME: %H:%M:%S"

# check if can skip building starting model
if [ -z "$MESA_RUN_OPTIONAL" ]; then
   cp standard_start.mod start.mod
else
   do_one inlist_start_header start.mod
   cp start.mod standard_start.mod
fi

do_one inlist_16M_conv_premix_header tams.mod

date "+DATE: %Y-%m-%d%nTIME: %H:%M:%S"

echo 'finished'

