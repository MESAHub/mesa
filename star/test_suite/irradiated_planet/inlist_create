      relax_irradiation_max_yrs_dt = 1d-1

      relax_to_this_irrad_flux = 1d7 ! erg s^-1 cm^-2
      irrad_col_depth = 1 ! g cm^-2

      relax_to_this_tau_factor = 10
      dlogtau_factor = 0.01d0
      relax_initial_tau_factor = .true.

      !pgstar_flag = .true.
      
/ ! end of star_job namelist


