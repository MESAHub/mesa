   include_P_in_velocity_time_centering = .true.
   include_L_in_velocity_time_centering = .true.

! turbulent energy
      !   et_alfa  =     1.5d0         ! mixing length; alfa = 0 gives a purely radiative model.
      !   et_alfap =     0.0d0         ! turbulent pressure; Pt ~ alfap     
      !   et_alfat =     0.0d0         ! turbulent flux; Lt ~ et_alfat
      !   et_alfam =     0.25d0        ! turbulent viscosity; Eq ~ et_alfam
      !   et_alfar =     0.0d0         ! radiative dissipation of turbulent energy; Dr ~ et_alfar   

! output
   
   !terminal_show_age_units = 'days'

   num_trace_history_values = 2
   trace_history_value_name(1) = 'rel_E_err'
   trace_history_value_name(2) = 'log_rel_run_E_err'

