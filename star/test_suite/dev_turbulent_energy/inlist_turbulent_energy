&controls  
   !et_Lsurf_factor = 0.25d0 ! for RSG and LBV

   initial_z = 0.02d0
   initial_mass = 30d0

   max_number_retries = 10
   max_model_number = 50

   max_age = 1d6
      
! wind

! atmosphere
   atm_option = 'T_tau'
   atm_T_tau_relation = 'Eddington'
   atm_T_tau_opacity = 'fixed'
   Pextra_factor = 2

! rotation

! mlt

! mixing

! timesteps
