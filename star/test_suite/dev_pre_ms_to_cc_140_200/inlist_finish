
&star_job
   
      load_saved_model = .true.
      saved_model_name = 'converted.mod'

      save_model_when_terminate = .true.
      save_model_filename = 'final.mod'
      required_termination_code_string = 'log_Rsurf_upper_limit'

      !pgstar_flag = .false.

/ ! end of star_job namelist

&eos
   use_Skye = .true.
/ ! end of eos namelist

&kap
/ ! end of kap namelist

&controls
   
      ! limit max_model_number as part of test_suite
      max_model_number = 10000      
      !max_number_retries = 30
      
      log_Rsurf_upper_limit = 4d0
      
      fe_core_infall_limit = 1d99
      non_fe_core_infall_limit = 1d99
      log_center_temp_limit = 1d99

      when_to_stop_rtol = 1d99
      when_to_stop_atol = 1d99

      gamma1_limit_max_q = 0.75d0

! trim envelope when radius gets too large
      !x_logical_ctrl(20) = .true. ! check for r/rsun > x_ctrl(19)
      x_ctrl(19) = 1000 ! trigger for R too large
      x_ctrl(20) = 5.0 ! cut back to place where v < v_esc*x_ctrl(20)
      x_ctrl(21) = 1000 ! or cut back to place where r/rsun < x_ctrl(21)


! wind

! atmosphere

! rotation

! mlt
      alpha_semiconvection = 0
      thermohaline_coeff = 0
      max_logT_for_mlt = 9.3d0

! mixing
      
! timesteps
      limit_for_rel_error_in_energy_conservation = 1d-4
      hard_limit_for_rel_error_in_energy_conservation = 1d-3

      delta_lgTeff_limit =  0.2
      delta_lgL_limit = 0.5     
      delta_lgL_He_limit = 0.5

      delta_lgT_max_limit = 0.03    
      delta_lgT_max_hard_limit = 0.1

      delta_lgT_cntr_limit = 0.03    
      delta_lgT_cntr_hard_limit = 0.1

      delta_XHe_cntr_limit = 0.03
      delta_XHe_cntr_hard_limit = 0.1

      delta_XSi_cntr_limit = 0.01d0
      delta_XSi_cntr_hard_limit = 0.03d0

      !dt_div_min_dr_div_cs_limit = 2 ! 0.7d0

! mesh

! solver
      use_gold2_tolerances = .false.
      use_gold_tolerances = .true.

      max_abs_rel_run_E_err = 1d99
      warn_when_large_rel_run_E_err = 1d99

      gold_iter_for_resid_tol3 = 7
      gold_tol_max_residual3 = 1d-4

      always_use_dedt_form_of_energy_eqn = .true.       
      use_dPrad_dm_form_of_T_gradient_eqn = .true.
      use_d_eos_dxa = .true.
      
      use_compression_outer_BC = .true.
      
      min_timestep_limit = 1d-12

      convergence_ignore_equL_residuals = .true. 
      make_gradr_sticky_in_solver_iters = .true.
      
      xa_scale = 1d-5
      iter_for_resid_tol2 = 10
      min_timestep_limit = 1d-12 ! (seconds)

      warn_rates_for_high_temp = .true.
      max_safe_logT_for_rates = 10.5d0

      screening_mode = 'chugunov'

      sig_min_factor_for_high_Tcenter = 0.01
      Tcenter_min_for_sig_min_factor_full_on = 3.2d9
      Tcenter_max_for_sig_min_factor_full_off = 2.8d9

      op_split_burn = .true.
      op_split_burn_min_T = 1d9
      burn_steps_limit = 150
      burn_steps_hard_limit = 250
      op_split_burn_eps = 1d-5
      op_split_burn_odescal = 1d-5

! GYRE output controls

      !write_pulse_data_with_profile = .true.
      pulse_data_format = 'GYRE'
      
      !x_logical_ctrl(37) = .true. ! if true, then run GYRE

      x_integer_ctrl(1) = 1 ! output GYRE info at this step interval
      x_logical_ctrl(1) = .false. ! save GYRE info whenever save profile

      x_integer_ctrl(2) = 2 ! max number of modes to output per call
      x_logical_ctrl(2) = .false. ! output eigenfunction files

      x_integer_ctrl(3) = 0 ! mode l (e.g. 0 for p modes, 1 for g modes)
         ! should match gyre.in mode l
      x_integer_ctrl(4) = 1 ! order
      x_ctrl(1) = 0.158d-05 ! freq ~ this (Hz)
      x_ctrl(2) = 0.33d+03 ! growth < this (days)

! output

      num_trace_history_values = 5
      trace_history_value_name(1) = 'non_fe_core_infall'
      trace_history_value_name(2) = 'fe_core_infall'
      trace_history_value_name(3) = 'bound_mass'
      trace_history_value_name(4) = 'rel_E_err'
      trace_history_value_name(5) = 'log_rel_run_E_err'
      
      !report_min_dr_div_cs = .true.

      !photo_interval = 10
      !profile_interval = 1
      !history_interval = 1
      !terminal_interval = 1

/ ! end of controls namelist

