   !Profile_Panels1_win_flag = .true.
   Profile_Panels1_win_width = 6
   Profile_Panels1_win_aspect_ratio = 1 ! aspect_ratio = height/width

   Profile_Panels1_title = 'Profile Panels'      
   Profile_Panels1_num_panels = 3  
         
