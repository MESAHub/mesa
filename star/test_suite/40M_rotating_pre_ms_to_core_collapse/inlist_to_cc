min_q_for_dt_div_min_dr_div_cs_limit = 0.005d0
max_q_for_dt_div_min_dr_div_cs_limit = 0.995d0!0.995d0!1d0!0.995d0
check_remnant_only_for_dt_div_min_dr_div_cs_limit = .false.


max_abs_du_div_cs_for_convection = 0.03d0 !5d-3
max_v_div_cs_for_convection = 1d2
max_v_for_convection = 1d4

! u_flag controls above



! to avoid a host of retries we relax our limits on split_burn
! for speed/stability in test_suite, at reduced time resolution.
!op_split_burn_min_T = 1d9

! prevent development of radial pulses during advanced burning
      !drag_coefficient = 1d0
      !min_q_for_drag = 0.8d0

      set_min_D_mix = .true.
      min_D_mix = 1d-2 

      fe_core_infall_limit = 5d7 ! 500 km/s infall.

      ! limit max_model_number as part of test_suite
      max_model_number = 6000
      !max_number_retries = 37

