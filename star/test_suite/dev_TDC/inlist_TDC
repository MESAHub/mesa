&controls  

   initial_z = 0.007d0
   initial_mass = 4.165d0

   max_model_number = 1000000
   max_number_retries = 10000

   max_age = 1d6
   max_timestep = 407.61 ! seconds for rsp.mod of Cepheid
      
! wind

! atmosphere
   atm_option = 'T_tau'
   atm_T_tau_relation = 'Eddington'
   atm_T_tau_opacity = 'fixed'
   Pextra_factor = 2

! rotation

! mlt

! mixing

! timesteps
   varcontrol_target = 1d-3 

! mesh
   okay_to_remesh = .false.

! TDC

   TDC_alfa  =     1.5d0         ! mixing length; alfa = 0 gives a purely radiative model.
   TDC_alfap =     0.0d0         ! turbulent pressure; Pt ~ alfap     
   TDC_alfat =     0.0d0         ! turbulent flux; Lt ~ TDC_alfat
   TDC_alfam =     0.25d0        ! turbulent viscosity; Eq ~ TDC_alfam
   TDC_alfar =     0.0d0         ! radiative dissipation of turbulent energy; Dr ~ TDC_alfar   

   TDC_num_outermost_cells_forced_nonturbulent = 1
   TDC_num_innermost_cells_forced_nonturbulent = 5
   
! solver   
   solver_use_lnR = .false.
   solver_use_lnT = .false.
   solver_use_lnd = .false.
   
   tol_correction_norm = 1d99 ! turn off when not using lnR/lnT/lnd
   tol_max_correction = 1d99

   always_use_dedt_form_of_energy_eqn = .true. ! no eps_grav

   eps_nuc_factor = 0
   non_nuc_neu_factor = 0
   dxdt_nuc_factor = 0
   mix_factor = 0

   set_rho_to_dm_div_dV = .false.
   
   steps_before_use_velocity_time_centering = 0
   include_P_in_velocity_time_centering = .true.
   include_L_in_velocity_time_centering = .true.
   use_Fraley_PdV_work_when_time_centering_velocity = .true.
   
   use_fixed_Psurf_outer_BC = .true.
   fixed_Psurf = 0

! output
   terminal_show_age_units = 'days'
   terminal_show_timestep_units = 'secs'
   terminal_show_log_dt = .false.
   terminal_show_log_age = .false.

   num_trace_history_values = 1
   trace_history_value_name(1) = 'log_rel_run_E_err'
   trace_history_value_name(2) = 'rel_E_err'

   photo_interval = 4000
   profile_interval = 4000
   history_interval = 10
   terminal_interval = 4000

      photo_interval = 1000
      profile_interval = 1000
