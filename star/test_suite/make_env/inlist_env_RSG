      set_initial_tau_factor = .true.
      set_to_this_tau_factor = 1d0

      profile_starting_model = .true.

      !pgstar_flag = .true.

/ ! end of star_job namelist

&eos
/ ! end of eos namelist

&kap
      Zbase = 0.02d0
      kap_file_prefix = 'gs98'
      use_Type2_opacities = .true.
/ ! end of kap namelist

&controls  

      x_character_ctrl(1) = 'basic.net'
      ! specify abundances
      initial_z = 0.02d0
      accrete_same_as_surface = .false.
      accretion_h1 = 0.68d0
      accretion_h2 = 0d0
      accretion_he3 = 0d0
      accretion_he4 = 0.30d0
      accretion_zfracs = 6
      
      x_integer_ctrl(1) = 1000 ! initial nz
      x_ctrl(5) = -4d0 ! -12.3d0 ! initial log(dq(1))
            
      x_logical_ctrl(1) = .true. ! build starting model
      ! 
         x_ctrl(1) = 20d0 ! star mass (Msun)
         x_ctrl(2) = 0.013d0 ! model mass as fraction of star mass
         x_logical_ctrl(2) = .false. ! get R using L = 4*pi*R^2*boltz_sigma*Teff**4
         x_ctrl(3) = 3500 ! Teff    3400 bad
         x_ctrl(4) = 1d5  ! L/Lsun
         x_ctrl(6) = 1d0 ! tau_factor for building env
   
      !photo_interval = 1
      !profile_interval = 1
      !history_interval = 1
      !terminal_interval = 1

! FOR DEBUGGING

      !report_solver_progress = .true. ! set true to see info about solver iterations
      !report_ierr = .true. ! if true, produce terminal output when have some internal error

      !solver_save_photo_call_number = 0
         ! Saves a photo when solver_call_number = solver_save_photo_call_number - 1
         ! e.g., useful for testing partials to set solver_call_number = solver_test_partials_call_number - 1

      !solver_test_partials_k = 131
      !solver_test_partials_call_number = 189
      !solver_test_partials_iter_number = 9
      !solver_test_partials_dx_0 = 1d-6
      !solver_test_partials_var_name = 'all' ! or 'lnd', 'lnT', 'lnR', 'L', 'v', etc.    '' means code sets 
      !solver_test_partials_equ_name = 'all' ! or 'dlnE_dt', 'dlnd_dt', 'dlnR_dt', etc   '' means code sets 
         ! equ name can also be 'lnE' to test eos, 'eps_nuc' to test net, or 'opacity' to test kap.
      !solver_test_partials_sink_name = 'he4' ! iso name to use for "sink" to keep sum = 1
         ! if possible, use he4 as sink so change Y rather than X or Z.
      !solver_test_partials_show_dx_var_name = 'h1'

      !solver_test_partials_write_eos_call_info = .true.
      
      !solver_test_partials_k_low = -1
      !solver_test_partials_k_high = -1
      
      !solver_test_eos_partials = .true.
      !solver_test_kap_partials = .true.
      !solver_test_net_partials = .true.
      !solver_test_atm_partials = .true.

      !report_why_dt_limits = .true.
      !report_all_dt_limits = .true.
      
      !show_mesh_changes = .true.
      !mesh_dump_call_number = 5189
      !okay_to_remesh = .false.
      
      !trace_evolve = .true.

      !stop_for_bad_nums = .true.
      



/ ! end of controls namelist



