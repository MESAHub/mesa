! inlist_env_Delta_Scuti

&star_job

      set_initial_tau_factor = .true.
      set_to_this_tau_factor = 1d-2

      !profile_starting_model = .true.

      !pgstar_flag = .true.

/ ! end of star_job namelist

&eos
/ ! end of eos namelist

&kap
      Zbase = 0.02d0
      kap_file_prefix = 'a09'
      kap_lowT_prefix = 'lowT_fa05_a09p'
      kap_CO_prefix = 'a09_co'
/ ! end of kap namelist

&controls  

      x_character_ctrl(1) = 'basic.net'
      ! specify abundances
      initial_z = 0.02d0
      accrete_same_as_surface = .false.
      accretion_h1 = 0.70d0
      accretion_h2 = 0d0
      accretion_he3 = 0d0
      accretion_he4 = 0.28d0
      accretion_zfracs = 6
      
      x_integer_ctrl(1) = 1500 ! initial nz
      x_ctrl(5) = -15d0 ! initial log(dq(1))
            
      x_logical_ctrl(1) = .true. ! build starting model
      ! rsp_Delta_Scuti 
         x_ctrl(1) = 2.0 ! star mass (Msun)
         x_ctrl(2) = 0.01d0 ! model mass as fraction of star mass
         !x_logical_ctrl(2) = .true. ! R and L in cgs units
         !x_ctrl(3) = 1d6 ! radius (cm)
         !x_ctrl(4) = 1d34 ! luminosity (erg/s)   use 1e35 for ns_c
         x_logical_ctrl(2) = .false. ! get R using L = 4*pi*R^2*boltz_sigma*Teff**4
         x_ctrl(3) = 6900 ! Teff
         x_ctrl(4) = 30d0 ! L/Lsun
         x_ctrl(6) = 0.8d0 ! tau_factor for building env
   
/ ! end of controls namelist



&pgstar
         
      ! top level controls

         !pause = .true. 
            ! if true, the code waits for user to enter a RETURN on the command line
      


/ ! end of pgstar namelist

