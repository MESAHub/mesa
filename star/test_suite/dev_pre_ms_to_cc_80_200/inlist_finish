&controls
      !save_controls_namelist = .true.
      !controls_namelist_name = 'controls_finish.txt'
   
      ! limit max_model_number as part of test_suite
      max_model_number = 10000      
      !max_number_retries = 30
      
      log_Rsurf_upper_limit = 7d0 ! for higher masses
      fe_core_infall_limit = 1d7 ! for lower masses
      bound_mass_min_limit = 10d0 ! for completely unbound cases
      
      when_to_stop_rtol = 1d99
      when_to_stop_atol = 1d99

      gamma1_limit_max_q = 0.75d0 ! only check below this limit when reporting min gamma1
      
      use_other_set_pgstar_controls = .true.

! tell run_star_extras that this is inlist_finish
      x_logical_ctrl(7) = .true.
      
! prune envelope when radius gets too large
      x_ctrl(19) = 3.5d0 ! limit for log(R/Rsun)
      x_ctrl(20) = 10.0d0 ! cut back to this v/v_esc
      
! switch between u_flag and v_flag depending on lgTmax
      x_ctrl(21) = 8.5d0 ! from u_flag to v_flag when drop below this
      x_ctrl(22) = 9.4d0 ! from v_flag to u_flag when go above this
      use_avQ_art_visc = .false. ! will automatically be set true for v_flag, false for u_flag
      avQ_cq = 4.0d0
      avQ_zsh = 0.0001d0
      
      x_ctrl(23) = 3.0 ! u_flag dt_div_min_dr_div_cs_limit
      x_ctrl(24) = 9.0 ! 1.5 ! u_flag dt_div_min_dr_div_cs_hard_limit      
      min_abs_du_div_cs_for_dt_div_min_dr_div_cs_limit = 1d0
      min_q_for_dt_div_min_dr_div_cs_limit = 0d0
      max_q_for_dt_div_min_dr_div_cs_limit = 1d0

! wind
    ! "rlo" wind scheme provides a simple radius-determined-wind with exponential increase.
    rlo_scaling_factor = 1
    rlo_wind_min_L = 1d-6 ! Only on when L > this limit. (Lsun)
    rlo_wind_max_Teff = 1d99 ! Only on when Teff < this limit.
    rlo_wind_roche_lobe_radius = 100 ! Only on when R > this (Rsun).
    rlo_wind_base_mdot = 1d-3 ! Base rate of mass loss when R = roche lobe radius (Msun/year).
    rlo_wind_scale_height = -1 ! Determines exponential growth rate of mass loss (Rsun).
    
    max_T_center_for_any_mass_loss = 9d9
    max_T_center_for_full_mass_loss = 7d9

! atmosphere

! rotation

! mlt

! mixing
      
! timesteps
      delta_lgTeff_limit =  0.2
      delta_lgL_limit = 0.5     
      delta_lgL_He_limit = 0.5

      delta_lgT_max_limit = 0.01    
      delta_lgT_max_hard_limit = 0.05

      delta_lgT_cntr_limit = 0.01    
      delta_lgT_cntr_hard_limit = 0.05

      delta_XHe_cntr_limit = 0.03
      delta_XHe_cntr_hard_limit = 0.1
 
      delta_XSi_cntr_limit = 0.01d0
      delta_XSi_cntr_hard_limit = -1

! mesh

! solver 
      use_compression_outer_BC = .true.

! output

      num_trace_history_values = 6
      trace_history_value_name(1) = 'non_fe_core_infall'
      trace_history_value_name(2) = 'fe_core_infall'
      trace_history_value_name(3) = 'bound_mass'
      trace_history_value_name(4) = 'rel_E_err'
      trace_history_value_name(5) = 'log_rel_run_E_err'
      trace_history_value_name(6) = 'fe_core_mass'

      photo_interval = 10
      !profile_interval = 1
      !history_interval = 1
      terminal_interval = 1

! FOR DEBUGGING

      !report_solver_progress = .true. ! set true to see info about solver iterations
      !report_ierr = .true. ! if true, produce terminal output when have some internal error
      !stop_for_bad_nums = .true.

      !solver_save_photo_call_number = 0
         ! Saves a photo when solver_call_number = solver_save_photo_call_number - 1
         ! e.g., useful for testing partials to set solver_call_number = solver_test_partials_call_number - 1

      !solver_test_partials_call_number = 464
      !solver_test_partials_k = 27
      !solver_test_partials_iter_number = 2
      !solver_test_partials_dx_0 = 1d-2
      !solver_test_partials_var_name = 'lnT' ! 'all' or 'lnd', 'lnT', 'lnR', 'L', 'v', etc.    '' means code sets 
      !solver_test_partials_equ_name = 'grad_ad' ! 'all' or 'dlnE_dt', 'dlnd_dt', 'dlnR_dt', 'equL', etc   '' means code sets 
      !solver_test_partials_sink_name = 'si28' ! iso name to use for "sink" to keep sum = 1
      !solver_test_partials_show_dx_var_name = 'h1'

         ! equ name can also be one of these
            ! 'lnE', 'lnP', 'grad_ad' to test eos
            ! 'eps_nuc' to test net
            ! 'non_nuc_neu' to test neu
            ! 'gradT', 'mlt_vc' to test mlt
            ! 'opacity' to test kap

      !solver_test_partials_write_eos_call_info = .true.
      
      !solver_test_partials_k_low = -1
      !solver_test_partials_k_high = -1
      
      !solver_test_eos_partials = .true.
      !solver_test_kap_partials = .true.
      !solver_test_net_partials = .true.
      !solver_test_atm_partials = .true.
      
      !show_mesh_changes = .true.
      !mesh_dump_call_number = 5189
      !okay_to_remesh = .false.
      
      !energy_conservation_dump_model_number = 546
      
      !trace_evolve = .true.
      
      !use_DGESVX_in_bcyclic = .true.
      !use_equilibration_in_DGESVX = .true.
      !report_min_rcond_from_DGESXV = .true.


      ! solver debugging
      !solver_check_everything = .true.
      
      !solver_epsder_struct = 1d-6
      !solver_epsder_chem = 1d-6

      !fill_arrays_with_NaNs = .true.
      
      !report_solver_dt_info = .true.
