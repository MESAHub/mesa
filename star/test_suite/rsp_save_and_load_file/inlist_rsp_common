         Grid2_plot_name(3) = 'Text_Summary1'
         Grid2_plot_row(3) = 7 ! number from 1 at top
         Grid2_plot_rowspan(3) = 2 ! plot spans this number of rows
         Grid2_plot_col(3) =  1 ! number from 1 at left
