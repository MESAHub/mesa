
&multi_stars_job

   num_stars = 2
   
   inlist_names(1) = 'inlist_star1_header' 
   inlist_names(2) = 'inlist_star2_header'
   
   ! for restart, set restart_flag true, set restart filenames, then ./rn (not ./re)
   
   ! NOTE: there may be problems with RSP restarts
   restart_flag = .true.
   restart_names(1) = '3000'
   restart_names(2) = '3000'
   
   which_for_pgstar = -1 ! do both
   
   stopping_age = 1.015e7
   
   stop_when_1st_finishes = .false.

/ ! end of multi_stars_job namelist
