&star_job
   create_pre_main_sequence_model = .false.

   save_model_when_terminate = .true.
   save_model_filename = 'final.mod'
   !required_termination_code_string = 'Successful test: evolved 100 days past first relax'

   load_saved_model = .true.
   load_model_filename = 'he_dep.mod'

   read_extra_star_job_inlist(1) = .true.
   extra_star_job_inlist_name(1) = 'inlist_ppisn'

   !change_initial_v_flag = .true.
   !new_v_flag = .true.

!relax_to_this_tau_factor = 1d-3!-1
!dlogtau_factor = 0.1d0
!relax_tau_factor = .true.

   
    set_initial_dt = .true.
    years_for_initial_dt = -1
    seconds_for_initial_dt = 1d5!-1



/ !end of star_job namelist


&controls


   ! in order for Riemann hydro to be turned on, the following two conditions need to be met
   x_ctrl(21) = 10d0 ! the integrated gamma1 must be smaller than this (min_gamma_sub_43_for_hydro)


   read_extra_controls_inlist(1) = .true.
   extra_controls_inlist_name(1)= 'inlist_ppisn'

   x_logical_ctrl(22) = .true. ! flag for in inlist_pulses

   ! See Farag et al 2022
   set_min_d_mix=.true.
   min_d_mix=1d-2

      MLT_option = 'TDC'
      alpha_TDC_DAMP = 1d0
      alpha_TDC_DAMPR = 1d0!1d0! 0d0 for mlt
      alpha_TDC_PtdVdt = 0d0!1d0!1d-2!1d-2!0d0
      steps_before_use_TDC = 0

   !v_drag is set in run_star_extras
   !q_for_v_drag_full_off = 0.98d0
   !q_for_v_drag_full_on = 0.99d0
   max_surface_cell_dq = 1d-12


      !drag_coefficient = 0.8d0
      !min_q_for_drag = 0.98d0

!Pextra_factor = 2
/ ! end of controls namelist

&pgstar

   read_extra_pgstar_inlist(2) = .true.
   extra_pgstar_inlist_name(2)= 'inlist_pgstar'


    History_Panels1_xaxis_name = 'yr_since_coll' ! need to change pulse condition for this.


         ! axis limits -- to override system default selections
         HR_logT_min = 3.5 !-101d0 ! only used if /= -101d0
         HR_logT_max = 4.5!-101d0 ! only used if /= -101d0
         HR_logL_min = 4.5!-101d0 ! only used if /= -101d0
         HR_logL_max = 5.6!-101d0 ! only used if /= -101d0


/ ! end of pgstar namelist
