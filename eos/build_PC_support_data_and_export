#!/bin/bash

function check_okay {
	if [ $? -ne 0 ]
	then
	   echo FAILED
		exit 1
	fi
}

echo 'create eosPC_support_data'

mkdir -p ../data/eosPC_support_data
check_okay

mkdir -p ../data/eosPC_support_data/cache
check_okay

rm -rf ../data/eosPC_support_data/*.data
rm -rf ../data/eosPC_support_data/cache/*.data